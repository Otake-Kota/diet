<div class="sign_in">
    <div class="contents">
        <div class="sign_in_from form">
            <h1 class="title">Sign in</h1>
            <%= form_for @user, url: sign_in_process_path do |f| %>
                <div>
                    <p>メールアドレス</p>
                    <%= f.email_field :email, placeholder: "diet@diet.jp" %>
                </div>
                <div>
                    <p>パスワード</p>
                    <%= f.password_field :password, placeholder: "８文字以上の英数字" %>
                </div>
                <button class="contents">サインイン</button>
            <% end %>
            <div class="link_sign_up">
                <%= link_to sign_up_path, :class => "new_sign_up" do %>
                >>sign up
                <% end %>
            </div>
        </div>
    </div>
</div>